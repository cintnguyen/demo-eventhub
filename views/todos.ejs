    <!-- HEADER-SECTION -->
    <div class="home-header-section sub-header-section">
        <header class="header">
            <div class="main-header">
                <div class="container">
                    <nav class="navbar navbar-expand-lg navbar-light p-0">
                        <!-- <a class="navbar-brand pt-0" href="index.html"><img src="../assets/images/bridely-logo.png"
