        <!-- BANNER-SECTION -->
        <div class="home-banner-section overflow-hidden position-relative sub-baner">
            <figure class="banner-img1 mb-0">
                <img src="../assets/images/banner-img1.png" alt="" class="star">
            </figure>
            <figure class="banner-img2 mb-0">
                <img src="../assets/images/banner-img2.png" alt="" class="star">
            </figure>
            <div class="banner-container-box">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 order-lg-2 order-1">
                            <div class="position-relative sub-banner-section" data-aos="fade-up" id="myContentDIV">
                                <h1><span class="h1-text"><%= eventName %> </span>To Do List</h1>
                                <p class="banner-paragraph">
                                    If you're not sure where to begin, use our ask AI feature!
                                </p>
                                <!-- <div class="banner-btn discover-btn-banner">
                                    <a href="about.html" class="text-decoration-none"><i
                                            class="fa fa-clipboard-list"></i>Make Reservations</a>
                                </div> -->
                                <!-- <div class="banner-btn discover-btn-banner">
                                    <div class="btn-inner">Home  -<span class="about-margin"> About</span></div>
                                </div> -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        
    <div class="counter-section">
       <div class="container">
            <div id="cntodolist" class="wedding-services">
                <figure class="mb-0 about-section-f1"><img src="../assets/images/about-bird-icon.png" alt="" class="star">
                </figure>
                <div class="row">
                    <div class="col-xl-5 col-lg-6 col-md-12 col-sm-12">
                        <div class="about-us-content">
                            <h5 class="autorix-text"><span class="ityped">Lets plan together!</span><span class="ityped-cursor">|</span></h5>
                            <h2>Ask AI:</h2>
                            <p class="aboutus-p">We will generate a to do list for you</p>
                            <% console.log(toDosItems.length?true:false) %>
                                <form action="/todos/<%=eventID%>" method="POST" id="chatGPTSearchToDos">
                                    <input type="hidden" id="toDoListExist" value="<%= toDosItems.length?true:false%>">
                                    <input type="text" name="eventName" placeholder="insert event type" >
                                    <button type="submit">Submit</button>
                                </form>
                            <br>
                            <h2>Or Enter In Your Own Task:</h2>
                                <form action="/todostwo/<%=eventID%>" method="POST">
                                    <input type="text" name="userAddTask" placeholder="add task" >
                                    <button type="submit">Submit</button>
                                </form>
                        </div>
                    </div>
                    <!-- <div class="col-lg-1 col-md-1 col-sm-1 d-xl-block d-none"></div> -->
                    <div class="col-xl-7 col-lg-6 col-md-12 col-sm-12">
                        <div class="about-content-img">
                         <h2>To Do List Items:</h2>
                         </br>
                         <ul class="list-unstyled mb-0">
                            <% for (let i = 0; i < toDosItems.length; i++) { %>
                             <li><h6><%= toDosItems[i].taskName %></h6>
                                <span class="serivices-percent">
                                    <form action="/todos/<%= eventID%>?_method=DELETE" method="POST">
                                        <input type="hidden" value="<%=i%>" name="task">
                                        <button type="submit" class="fa fa-trash icons-form" ></button>
                                    </form>
                                </span>
                                <!-- <div class="progress">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100" style="width: 85%;">
                                      <span class="sr-only">85% Complete</span>
                                    </div>
                                </div> -->
                             </li>
                            <% } %>
                         </ul>
                         <a href="/todos/<%=eventID%>/clearAll">Clear All</a>
                        </div>
                    </div>
                </div>
            </div>
       </div>
    </div>
    
    <section class="video-section-about">
        <div class="container">
        </div>
    </section>
      
